n1 = float(input('Digite a primeira nota: '))
n2 = float(input('Digite a segunda nota: '))
média = (n1 + n2) /2.0
print(f'A média do aluno foi: {média}')
if 9.0 <= média <= 10.0:
    print('Conceito da nota A\nAPROVADO')
elif 7.5 <= média < 9.0:
    print('Conceito nota B\nAPROVADO')
elif 6.0 <= média < 7.5:
    print('Conceito nota C\nAPROVADO')
elif 4.0 <= média < 6.0:
    print('Conceito nota D\nREPROVADO')
else:
    print('Conceito nota E\nREPROVADO')